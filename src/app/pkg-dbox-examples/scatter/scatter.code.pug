div#chart
script.
  var config = {
    'bindTo': '#chart',
    'size':{
      'width':960,
      'height':500,
      'margin':{top: 20, right: 20, bottom: 30, left: 40},
    },
    'xAxis':{
      'scale' : 'linear'
    },
    'yAxis': {
      'scale' : 'linear'
    }
  }

  var chart = dbox.chart(config)
                .data({'tsv':'app/pkg-dbox-examples/data/data.tsv'})
              .layer(dbox.scatter)
                .x('sepalWidth')
                .color('species')
                .y('sepalLength')
                .tip(function(d){
                  console.log(d);
                  return 'x: ' + d.x + ', y:' + d.y;
                })
              .end()
                .draw();
